#include <iostream>
using namespace std;

double h,b,f1,f2;
int n, j,i;
double a[3]{};


int main()
{
    cout << "enter h=" << endl;
    cin >> h;
    cout << "enter b=" << endl;
    cin >> b;
    cout << "enter n=" << endl;
    cin >> n;
    double* x = new double[n];
    cout << "enter a[3]=" << endl;
    cin >> a[0] >> a[1] >> a[2];
    for (j = 1; j < n; j++) {
        x[j] = b + j * h;
        for (i = 0; i < 3; i++) {
            if (a[i] >= 0) {
                f1 = x[j] * cos(a[i] - x[j] + 1);
                cout << f1 << endl;
            }
            else {
                f2 = 2 * cos(a[i] + x[j]);
                cout << f2 << endl;
            }
        }
    }
}

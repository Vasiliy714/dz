#include <iostream>
using namespace std;

int a, b, c, d,i,t=0;

int main()
{
    cin >> a;
    cin >> b;
    cin >> c;
    cin >> d;
    for (i = 10000; i < 100000;i++) {
        if ((i % a == b) && (i % c == d)) {
            cout << i << " ";
            t = 1;
        }
        }
    if (t == 0) {
        cout << -1;
    }
    
}

#include <iostream>
using namespace std;

int n,i,t=0;

int main()
{
    cin >> n;
    for (i = 2; i < sqrt(n) && t==0;i++) {
        if (n%i==0) {
            t = 1;
        }
        }
    if (t == 0) {
        cout << "prime";
    }
    if (t == 1) {
        cout << "composite";
    }
    
}

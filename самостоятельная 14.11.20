#include <iostream>
using namespace std;

int i,t=0,m=0,f=0;
double o;
int const n = 7;
int a[n];

int main()
{
    for (i = 0; i < n; i++) {
        cout << "Enter a[" << i << "]=";
        cin >> a[i];
    }
    for (i = 0; i < n; i++) {
        if (a[i] % 2 == 1) {
            t++;
            m += a[i];
            f = 1;
        }
    }
    if (f == 0) {
        cout << "NO";
    }
    else {
        o = m / t;
        cout << o;
    }
}
